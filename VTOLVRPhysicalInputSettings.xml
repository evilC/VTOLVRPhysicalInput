<?xml version="1.0" encoding="utf-8" ?>
<Mappings>
  <StickMappings>
    <StickName>T16000M</StickName>
    <AxisToVectorComponent>
      <InputAxis>X</InputAxis>
      <Invert>true</Invert>
      <OutputDevice>Stick</OutputDevice>
      <OutputComponent>z</OutputComponent>
    </AxisToVectorComponent>
    <AxisToVectorComponent>
      <InputAxis>Y</InputAxis>
      <Invert>true</Invert>
      <OutputDevice>Stick</OutputDevice>
      <OutputComponent>x</OutputComponent>
    </AxisToVectorComponent>
    <ButtonToButton>
      <Name>B2B1</Name>
    </ButtonToButton>
  </StickMappings>

  <StickMappings>
    <StickName>TWCS Throttle</StickName>
    <AxisToVectorComponent>
      <InputAxis>RotationZ</InputAxis>
      <Invert>false</Invert>
      <OutputDevice>Stick</OutputDevice>
      <OutputComponent>y</OutputComponent>
    </AxisToVectorComponent>
    <AxisToFloat>
      <InputAxis>Z</InputAxis>
      <Invert>true</Invert>
      <OutputDevice>Throttle</OutputDevice>
    </AxisToFloat>
    <ButtonToButton>
      <Name>B2B1</Name>
    </ButtonToButton>

  </StickMappings>
</Mappings>
